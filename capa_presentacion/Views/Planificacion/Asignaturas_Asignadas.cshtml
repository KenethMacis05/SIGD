﻿
@{
    ViewBag.Title = "Asignaturas asignadas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!--Titulo-->
<div class="alert text-center" style="background-color: #F7F7F7; border: 1px #D2D2D2 solid " role="alert"><h2 class="fs-3">@ViewBag.Title</h2></div>

<!--Contenedor-->
<div class="card">
    <div class="card-body">
        <!--Tabla-->
        <table class="display cell-border" style="width: 100%" id="datatable">
            <thead class="table-cabezera cell-border" style="background: #0072BB; color: white">
                <tr>
                    <th>#</th>
                    <th>Codigo</th>
                    <th>Nombre</th>
                    <th>Propietario</th>
                    <th>Asignatura</th>
                    <th># de Semanas</th>
                    <th>Estado</th>
                    <th>Asignada</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
            <tfoot>

            </tfoot>
        </table>
    </div>
</div>

@section scripts{
    <script src="@Url.Content("~/Scripts/Views/AsignaturasAsignadas.js")" type="text/javascript"></script>
}